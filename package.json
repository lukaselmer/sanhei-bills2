{
  "name": "sanhei-bills2",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "deploy-main": "firebase use main && firebase deploy",
    "ng": "ng",
    "start": "ng serve --host sanhei-bills.test --open",
    "ci": "yarn run ci-before-build && yarn run build-prod",
    "ci-before-build": "yarn run lint && yarn run ci-test",
    "build": "ng build",
    "build-prod": "ng build --configuration production --progress false",
    "build-watch": "ng build --watch",
    "lint": "yarn run ng-lint && yarn run prettier",
    "ng-lint": "ng lint --fix true",
    "prettier": "prettier --list-different --write '**/*.{ts,js,json,md,scss}'",
    "e2e": "ng e2e",
    "test": "ng test",
    "ci-test": "ng test --watch=false --progress false --code-coverage",
    "update-browserlist": "browserslist --update-db"
  },
  "private": false,
  "repository": "lukaselmer/sanhei-bills2",
  "dependencies": {
